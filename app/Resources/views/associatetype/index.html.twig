{% extends 'base.html.twig' %}

{% block body %}
    <h1>Associatetypes list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for associateType in associateTypes %}
            <tr>
                <td><a href="{{ path('associatetype_show', { 'id': associateType.id }) }}">{{ associateType.id }}</a></td>
                <td>{{ associateType.name }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('associatetype_show', { 'id': associateType.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('associatetype_edit', { 'id': associateType.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('associatetype_new') }}">Create a new associateType</a>
        </li>
    </ul>
{% endblock %}
